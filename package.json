{
  "name": "semantic-pullrequest-action",
  "version": "1.6.1",
  "private": true,
  "description": "this action checks the commits and the title of a pr via commitlint",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paschdan/semantic-pullrequest-action"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@commitlint/format": "^15.0.0",
    "@commitlint/lint": "^15.0.0",
    "@commitlint/load": "^15.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vercel/ncc": "^0.33.0",
    "commitlint-plugin-function-rules": "^1.5.1",
    "eslint": "^8.4.1",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^25.3.0",
    "jest": "^27.4.3",
    "jest-circus": "^27.4.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.0",
    "typescript": "^4.5.2"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/changelog",
      "@semantic-release/git"
    ]
  }
}
