{
  "name": "semantic-pullrequest-action",
  "version": "1.3.0",
  "private": true,
  "description": "this action checks the commits and the title of a pr via commitlint",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paschdan/semantic-pullrequest-action"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.7",
    "@actions/github": "^4.0.0",
    "@commitlint/config-conventional": "^12.1.4",
    "@commitlint/format": "^12.1.1",
    "@commitlint/lint": "^12.1.1",
    "@commitlint/load": "^12.1.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.3",
    "@typescript-eslint/parser": "^4.23.0",
    "@vercel/ncc": "^0.28.5",
    "commitlint-plugin-function-rules": "^1.3.2",
    "eslint": "^7.26.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/changelog",
      "@semantic-release/git"
    ]
  }
}
