{
  "name": "semantic-pullrequest-action",
  "version": "1.6.2",
  "private": true,
  "description": "this action checks the commits and the title of a pr via commitlint",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paschdan/semantic-pullrequest-action"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.7.0",
    "@actions/github": "^5.0.1",
    "@commitlint/config-conventional": "^16.2.4",
    "@commitlint/format": "^16.2.1",
    "@commitlint/lint": "^16.2.1",
    "@commitlint/load": "^16.2.3"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vercel/ncc": "^0.33.4",
    "commitlint-plugin-function-rules": "^1.6.2",
    "eslint": "^8.14.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.1.5",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "2.6.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/changelog",
      "@semantic-release/git"
    ]
  }
}
